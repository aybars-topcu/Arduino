// Knight Rider :)

int leds[] = {2,3,4,5,6};

void setup()
{
  for(int i=0;i<5;i++){
    pinMode(leds[i],OUTPUT);
  }
}

void loop()
{
  for(int i=0;i<5;i++) {
    digitalWrite(leds[i],HIGH);
    delay(80);
    digitalWrite(leds[i],LOW);
   }
  for(int j=4;j>-1;j--) {
  digitalWrite(leds[j],HIGH);
  delay(80);
  digitalWrite(leds[j],LOW);
  }
}  
